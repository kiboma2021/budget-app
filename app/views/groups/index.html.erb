<p style="color: green"><%= notice %></p>


<div id="groups">
  <% @groups.each do |group| %>
    <div class="container" >
      <div class="d-flex justify-content-between p-2">
        <div class="d-flex justify-content-between p-2">
          <p> <%= group.icon %> </p>
          <p> <%= group.name %> </p>
        </div>
        <p> <%= group.name %> </p>
      </div>
    </div>
  <% end %>
</div>
